                },
                "require": {
                    "php": ">=8.1",
                    "rdlowrey/auryn": "^1.4",
                    "symfony/http-foundation": "^6.1",
                    "symfony/routing": "^6.1"
                },
                "require-dev": {
                    "brain/monkey": "^2.6",
                    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
                    "php-stubs/wp-cli-stubs": "^2.7",
                    "phpstan/extension-installer": "^1.2",
                    "phpstan/phpstan": "^1.9",
                    "phpunit/phpunit": "^9.5",
                    "squizlabs/php_codesniffer": "^3.7",
                    "szepeviktor/phpstan-wordpress": "^1.1"
                },
                "time": "2023-01-06T12:14:59+00:00",
                "default-branch": true,
                "type": "library",
                "autoload": {
                    "psr-4": {
                        "CEKW\\WpPlugin\\": "src/"
